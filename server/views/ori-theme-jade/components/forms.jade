doctype html
html
  head
    meta(charset='utf-8')
    // Always force latest IE rendering engine or request Chrome Frame
    meta(content='IE=edge,chrome=1', http-equiv='X-UA-Compatible')
    // Use title if it's in the page YAML frontmatter
    title Whitemin Theme
    link(href='../../stylesheets/application.css', media='all', rel='stylesheet', type='text/css')
    link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,800')
    script(src='../../javascripts/application.js', type='text/javascript')
    meta(name='viewport', content='width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0')
    // Le HTML5 shim, for IE6-8 support of HTML5 elements
    // there's an IE separated stylesheet with the following resets for IE
    //if lte IE 9
      script(src='http://html5shim.googlecode.com/svn/trunk/html5.js')
      script(src='../../javascripts/html5shiv.js', type='text/javascript')
      script(src='../../javascripts/excanvas.js', type='text/javascript')
      script(src='../../javascripts/ie_fix.js', type='text/javascript')
      link(href='../../stylesheets/ie_fix.css', media='screen', rel='stylesheet', type='text/css')
  body
    nav#primary.main-nav.collapsible-nav
      // Brand
      a#sidebar-brand(href='../pages/dashboard.html')
        | Whitemin
      button.btn.btn-navbar.btn-inverse(type='button', data-toggle='collapse', data-target='.nav-collapse')
        i.icon-reorder
      ul.nav-collapse.collapse
        // Profile
        li.profile
          a.accordion-toggle(href='#profile-opts', data-toggle='collapse')
            img.avatar.user3
            span#profile-text.text
              | Jane Doe
              b.caret
          ul#profile-opts.collapse.secondary
            li
              a(href='#')
                | Settings
            li
              a(href='#')
                | Change password
            li
              a(href='#')
                | Logout
        // Dashboard
        li
          a(href='../dashboard.html')
            span.icon
              i.icon-dashboard
            span.text
              | Dashboard
        // Components
        li.active
          a.accordion-toggle(href='#components-list', data-toggle='collapse')
            span.icon
              i.icon-microformats
            span.text
              | Components
              b.caret
          ul#components-list.collapse.secondary
            li
              a(href='buttons.html')
                | Buttons
            li.active
              a(href='forms.html')
                | Forms
            li
              a(href='elements.html')
                | Elements
            li
              a(href='widgets.html')
                | Widgets
            li
              a(href='charts.html')
                | Charts
            li
              a(href='tables.html')
                | Tables
            li
              a(href='grid.html')
                | Grid
        // Sample pages
        li
          a.accordion-toggle(href='#sample-pages', data-toggle='collapse')
            span.icon
              i.icon-pushpin
            span.text
              | Pages
              b.caret
          ul#sample-pages.collapse.secondary
            li
              a(href='../samples/gallery.html')
                | Gallery
            li
              a(href='../samples/faq.html')
                | FAQ
            li
              a(href='../samples/pricing_plans.html')
                | Pricing Plans
            li
              a(href='../samples/login.html')
                | Login
            li
              a(href='../samples/register.html')
                | Register
            li
              a(href='../samples/reports.html')
                | Reports
            li
              a(href='../samples/error.html')
                | Error 404
        // Extras dropdown
        li
          a.accordion-toggle(href='#extras', data-toggle='collapse')
            span.icon
              i.icon-gift
            span.text
              | Extras
              b.caret
          ul#extras.collapse.secondary
            li
              a(href='../extras/fullcalendar.html')
                | Fullcalendar
            li
              a(href='../extras/icons.html')
                | Icons
            li
              a(href='../extras/world-map.html')
                | World map
    .top-bar
      a#topbar-brand.brand(href='../dashboard.html')
        | Whitemin
      ul.top-notifications
        li.dropdown
          a.dropdown-toggle(data-toggle='dropdown', href='#')
            | Messages
            span.badge.dark-blue 3
          ul#messages.dropdown-menu.top-bar-list
            li
              a New message
            li
              a
                | Inbox
                span.count &lpar;8&rpar;
            li
              a Sent
            li
              a Drafts
        li.dropdown
          a.dropdown-toggle(data-toggle='dropdown', href='#')
            | Orders
            span.badge.dark-plum 45
          ul#orders.dropdown-menu.top-bar-list
            li
              a
                | New orders
                span.count &lpar;6&rpar;
            li
              a Refunds
            li
              a History
    section#main
      .row-fluid
        .span12
          hr.main
          ul.breadcrumbs
            li
              a.crumb(href='#') Some
              span.divider &sol;
            li
              a.crumb(href='#') Pretty
              span.divider &sol;
            li
              a.crumb(href='#') Cool
              span.divider &sol;
            li
              a.crumb.active(href='#') Crumbs
          hr.main
      .container-fluid
        .row-fluid
          .span12
            .box.bordered
              .box-header
                i.icon-th-list
                | Form elements
                .pull-right
                  .btn-group
                    a.dropdown-toggle(data-toggle='dropdown')
                      i.icon-cog
                    ul.dropdown-menu.dropdown-left
                      li
                        a Action
                      li
                        a Another action
                      li
                        a Something else here
                      li.divider
                      li
                        a Separated link
              .row-fluid
                .span6
                  .padded
                    .section-title
                      | Inputs
                    .vpadded
                      .input.full-width
                        input(type='text', placeholder='Username')
                      .input.full-width
                        input(type='text', placeholder='Email address')
                      .input.full-width
                        input(type='text', placeholder='First name')
                      .input.full-width
                        input(type='text', placeholder='Last name')
                      .note
                        i.icon-exclamation-sign
                        | You can write max 2500 characters&period;
                      .input.full-width
                        textarea(rows='5', placeholder='Your story...')
                    .section-title
                      | Validations
                    .vpadded
                      .warning.full-width
                        .input
                          input#inputWarning.warning(type='text')
                          span.input-warning(data-original-title='')
                            | Warning
                            i.icon-warning-sign
                      .error.full-width
                        .input
                          input#inputError.error(type='text')
                          span.input-error(data-original-title='')
                            | Error
                            i.icon-exclamation-sign
                      .success.full-width
                        .input
                          input#inputSuccess.success(type='text')
                          span.input-success(data-original-title='')
                            | Success
                            i.icon-ok-sign
                .span6
                  .padded
                    .section-title
                      | File uploaders
                    .vpadded
                      .input.full-width
                        .uploader
                          span.filename No file selected
                          span.action &plus;
                          input(type='file')
                      .input.full-width
                        .uploader
                          span.filename Upload a file
                          span.action
                            i.icon-circle-arrow-up
                          input(type='file')
                      .note
                        i.icon-exclamation-sign
                        | Please select a file to upload&period;
                      .input.full-width
                        .uploader
                          span.filename No file selected
                          span.action
                            i.icon-double-angle-up
                          input(type='file')
                    .section-title
                      | Selects
                    .vpadded
                      .full-width
                        select.chzn-select(data-placeholder='Choose a city...')
                          option(value='')
                          option Amsterdam
                          option Bucharest
                          option Cairo
                          option London
                          option Paris
                        select.chzn-select(data-placeholder='Choose a country...', multiple='')
                          option Australia
                          option Alaska
                          option Belize
                          option Greece
                          option Madagascar
              .row-fluid
                .span6
                  .padded
                    .section-title
                      | Search inputs
                    .vpadded
                      .input-append
                        input#appendedInputButton(type='text', size='16')
                        button.btn.light-blue.inverse(type='button') Go&excl;
                      .input-append
                        input#appendedInputButtons(type='text', size='16')
                        button.btn.light-blue.inverse(type='button') Search
                      form.form-search
                        .input-append
                          input.search-query(type='text')
                          button.btn.medium-blue(type='submit') Search
                        .input-prepend
                          button.btn.medium-blue(type='submit') Search
                          input.search-query(type='text')
                .span6
                  .padded
                    .section-title
                      | Appended and prepended inputs
                    .vpadded
                      .input-prepend
                        span.add-on &commat;
                        input#prependedInput(type='text', placeholder='Username')
                      .input-append
                        input#appendedInput(type='text')
                        span.add-on &period;00
                      .input-prepend.input-append
                        span.add-on &dollar;
                        input#appendedPrependedInput(type='text')
                        span.add-on &period;00
              .row-fluid
                .span6
                  .padded
                    .section-title
                      | Checkboxes
                    .vpadded
                      label.checkbox
                        input(type='checkbox', value='', checked='')
                        | Option one
                      label.checkbox
                        input(type='checkbox', value='')
                        | Option two
                      label.checkbox
                        input(type='checkbox', value='')
                        | Option three
                    .vpadded
                      label.checkbox.inline
                        input(type='checkbox', value='option1')
                        | Some
                      label.checkbox.inline
                        input(type='checkbox', value='option2')
                        | Inline
                      label.checkbox.inline
                        input(type='checkbox', value='option3')
                        | Checkboxes
                    .vpadded
                      div
                        .btn-group(data-toggle='buttons-checkbox')
                          button.btn.btn-small.light-blue.inverse(type='button') Check
                          button.btn.btn-small.light-blue.inverse(type='button') This
                          button.btn.btn-small.light-blue.inverse(type='button') Out
                      div
                        .btn-group(data-toggle='buttons-checkbox')
                          button.btn.btn-small.medium-blue(type='button') Option 1
                          button.btn.btn-small.medium-blue(type='button') Option 2
                          button.btn.btn-small.medium-blue(type='button') Option 3
                .span6
                  .padded
                    .section-title
                      | Radios
                    .vpadded
                      label.radio
                        input#optionsRadios1(type='radio', name='optionsRadios', value='option1', checked='')
                        | Option one
                      label.radio
                        input#optionsRadios2(type='radio', name='optionsRadios', value='option2')
                        | Option two
                      label.radio
                        input#optionsRadios3(type='radio', name='optionsRadios', value='option2')
                        | Option three
                    .vpadded
                      label.radio.inline
                        input#opt1(type='radio', name='optionsRadios', value='option1', checked='')
                        | Some
                      label.radio.inline
                        input#opt2(type='radio', name='optionsRadios', value='option2')
                        | Inline
                      label.radio.inline
                        input#opt3(type='radio', name='optionsRadios', value='option2')
                        | Radios
                    .vpadded
                      div
                        .btn-group(data-toggle='buttons-radio')
                          button.btn.btn-small.dark-blue(type='button') Check
                          button.btn.btn-small.dark-blue(type='button') This
                          button.btn.btn-small.dark-blue(type='button') Out
                      div
                        .btn-group(data-toggle='buttons-radio')
                          button.btn.btn-small.medium-blue(type='button') Option 1
                          button.btn.btn-small.medium-blue(type='button') Option 2
                          button.btn.btn-small.medium-blue(type='button') Option 3
      .container-fluid
        .row-fluid
          .span12
            .box.footer
              .copyright.text-transparent.center
                div 2013 &copy; Whitemin Template
                div Created by Adritha84
    // Modal dialog used for the image gallery
    #modal-gallery.modal.modal-gallery.hide.fade(tabindex='-1')
      .modal-header
        a.close(data-dismiss='modal') &times;
        h3.modal-title
      .modal-body
        .modal-image
      .modal-footer
        a.btn.modal-download.light-blue.inverse(target='_blank')
          i.icon-download
          | Download
        a.btn.modal-play.modal-slideshow(data-slideshow='5000')
          i.icon-play.icon-white
          | Slideshow
        a.btn.modal-prev.medium-blue
          i.icon-arrow-left.icon-white
          | Previous
        a.btn.modal-next.medium-blue
          | Next
          i.icon-arrow-right.icon-white
